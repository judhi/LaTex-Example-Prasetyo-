@article{ogren|fiorelli|leonard:2004, 
  author      = {Ogren, P. and Fiorelli, E. and Leonard, N.E.}, 
  journal     = {IEEE Transactions on Automatic Control}, 
  title       = {Cooperative control of mobile sensor networks: 
